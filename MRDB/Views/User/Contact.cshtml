@model IEnumerable<Mensajes_Relevantes.Models.User>

@{
    ViewData["Title"] = "Contact";
}
<p>Conecting: @ViewBag.sessionv</p>
<h1 class="title1">Contact</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nick_Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nick_Name)
            </td>
            <td>
                @Html.ActionLink("Add Friend", "Contact", item)
            </td>
        </tr>
}
    </tbody>
</table>
